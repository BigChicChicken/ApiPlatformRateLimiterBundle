{
    "name": "bigchicchicken/api-platform-rate-limiter-bundle",
    "type": "api-platform-bundle",
    "description": "Bundle to controls how frequently certain APIs are allowed to be called for ApiPlatform",
    "keywords": ["symfony", "api-patform", "rate limiter", "security"],
    "homepage": "https://github.com/BigChicChicken/ApiPlatformRateLimiterBundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ApiPlatformRateLimiterBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiPlatformRateLimiterBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Florent TEDESCO"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/framework-bundle": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/rate-limiter": "^6.1",
        "symfony/yaml": "^6.1",
        "api-platform/core": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/browser-kit": "^6.1",
        "symfony/http-client": "^6.1"
    }
}
